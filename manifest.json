{
  "manifest_version": 3,
  "name": "No Grayscale",
  "version": "1.0",
  "description": "Removes CSS grayscale filter from websites.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start" 
    }
  ]
}